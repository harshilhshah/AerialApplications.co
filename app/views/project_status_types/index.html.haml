%h1 Listing project_status_types

%table
  %thead
    %tr
      %th Description
      %th Name
      %th
      %th
      %th

  %tbody
    - @project_status_types.each do |project_status_type|
      %tr
        %td= project_status_type.description
        %td= project_status_type.name
        %td= link_to 'Show', project_status_type
        %td= link_to 'Edit', edit_project_status_type_path(project_status_type)
        %td= link_to 'Destroy', project_status_type, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Project status type', new_project_status_type_path
