%h1 Listing interactions

%table
  %thead
    %tr
      %th User
      %th Url
      %th Type
      %th
      %th
      %th

  %tbody
    - @interactions.each do |interaction|
      %tr
        %td= interaction.user
        %td= interaction.url
        %td= interaction.type
        %td= link_to 'Show', interaction
        %td= link_to 'Edit', edit_interaction_path(interaction)
        %td= link_to 'Destroy', interaction, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Interaction', new_interaction_path
