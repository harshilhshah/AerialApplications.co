= form_for @interaction do |f|
  - if @interaction.errors.any?
    #error_explanation
      %h2= "#{pluralize(@interaction.errors.count, "error")} prohibited this interaction from being saved:"
      %ul
        - @interaction.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user
    = f.number_field :user
  .field
    = f.label :url
    = f.text_field :url
  .field
    = f.label :type
    = f.text_field :type
  .actions
    = f.submit 'Save'
