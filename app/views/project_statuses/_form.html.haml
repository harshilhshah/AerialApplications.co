= form_for @project_status do |f|
  - if @project_status.errors.any?
    #error_explanation
      %h2= "#{pluralize(@project_status.errors.count, "error")} prohibited this project_status from being saved:"
      %ul
        - @project_status.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :projectId
    = f.number_field :projectId
  .field
    = f.label :owner
    = f.number_field :owner
  .field
    = f.label :statusId
    = f.number_field :statusId
  .field
    = f.label :comment
    = f.text_field :comment
  .actions
    = f.submit 'Save'
